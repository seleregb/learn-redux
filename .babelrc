{
  "presets": [
    "env"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        [
          "@babel/plugin-proposal-object-rest-spread"
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "@babel/plugin-proposal-object-rest-spread"
        ]
      ]
    }
  }
}